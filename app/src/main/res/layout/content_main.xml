<?xml version="1.0" encoding="utf-8"?>
    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:background="@color/snow"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:context="com.codenerds.zerocool.geronimo13.MainActivity"
        tools:showIn="@layout/app_bar_main">

    <ImageView
        android:id="@+id/iv_ironTile"
        android:layout_width="180dp"
        android:layout_height="225dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="70dp"
        app:srcCompat="@drawable/tile" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="180dp"
        android:layout_height="225dp"
        android:layout_above="@+id/imageView11"
        android:layout_alignLeft="@+id/imageView12"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignStart="@+id/imageView12"
        app:srcCompat="@drawable/tile" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="180dp"
        android:layout_height="225dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/iv_ironTile"
        android:layout_marginTop="14dp"
        app:srcCompat="@drawable/tile" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="180dp"
        android:layout_height="225dp"
        android:layout_alignBottom="@+id/textView15"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        app:srcCompat="@drawable/tile" />

        <ImageView
            android:id="@+id/imageView13"
            android:layout_width="90dp"
            android:layout_height="60dp"
            app:srcCompat="@drawable/iron"
            android:layout_marginLeft="45dp"
            android:layout_marginStart="45dp"
            android:layout_alignTop="@+id/iv_washingMachine"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignBottom="@+id/iv_washingMachine" />

        <ImageView
            android:id="@+id/iv_washingMachine"
            android:layout_width="110dp"
            android:layout_height="80dp"
            android:layout_marginTop="53dp"
            app:srcCompat="@drawable/wash___iron"
            android:layout_alignTop="@+id/imageView10"
            android:layout_alignLeft="@+id/imageView10"
            android:layout_alignStart="@+id/imageView10"
            android:layout_marginLeft="35dp"
            android:layout_marginStart="35dp" />

        <ImageView
            android:id="@+id/imageView15"
            android:layout_width="90dp"
            android:layout_height="60dp"
            android:layout_alignLeft="@+id/imageView13"
            android:layout_alignStart="@+id/imageView13"
            android:layout_alignTop="@+id/imageView11"
            android:layout_marginTop="75dp"
            app:srcCompat="@drawable/dry_cleaning" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/textView7"
            android:layout_alignEnd="@+id/imageView13"
            android:layout_alignLeft="@+id/imageView13"
            android:layout_alignRight="@+id/imageView13"
            android:layout_alignStart="@+id/imageView13"
            android:gravity="center"
            android:text="Only Iron"
            android:textSize="14sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView9"
            android:layout_alignEnd="@+id/textView6"
            android:layout_alignLeft="@+id/textView6"
            android:layout_alignRight="@+id/textView6"
            android:layout_alignStart="@+id/textView6"
            android:gravity="center"
            android:text="1 Day" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageView10"
            android:layout_alignEnd="@+id/textView8"
            android:layout_alignLeft="@+id/textView8"
            android:layout_alignRight="@+id/textView8"
            android:layout_alignStart="@+id/textView8"
            android:gravity="center"
            android:text="1 Day" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/imageView11"
            android:layout_alignEnd="@+id/textView6"
            android:layout_alignRight="@+id/textView6"
            android:layout_below="@+id/iv_ironTile"
            android:layout_marginEnd="22dp"
            android:layout_marginRight="22dp"
            android:text="1 Day" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageView11"
            android:layout_alignEnd="@+id/textView10"
            android:layout_alignLeft="@+id/textView10"
            android:layout_alignRight="@+id/textView10"
            android:layout_alignStart="@+id/textView10"
            android:gravity="center"
            android:text="2 Days" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/textView6"
            android:layout_alignBottom="@+id/textView6"
            android:layout_alignEnd="@+id/iv_washingMachine"
            android:layout_alignLeft="@+id/iv_washingMachine"
            android:layout_alignRight="@+id/iv_washingMachine"
            android:layout_alignStart="@+id/iv_washingMachine"
            android:gravity="center"
            android:text="Wash n Iron"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/textView11"
            android:layout_alignEnd="@+id/imageView15"
            android:layout_alignLeft="@+id/imageView15"
            android:layout_alignRight="@+id/imageView15"
            android:layout_alignStart="@+id/imageView15"
            android:gravity="center"
            android:text="Dry Cleaning"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/textView11"
            android:layout_alignEnd="@+id/textView13"
            android:layout_alignRight="@+id/textView13"
            android:layout_marginEnd="13dp"
            android:layout_marginRight="13dp"
            android:gravity="center"
            android:text="Subscription"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/textView14"
            android:layout_alignLeft="@+id/textView14"
            android:layout_alignRight="@+id/textView14"
            android:layout_alignStart="@+id/textView14"
            android:layout_below="@+id/textView14"
            android:gravity="center"
            android:text="2 Days" />

    <Button
        android:id="@+id/button_laundry"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:layout_alignBottom="@+id/textView9"
        android:layout_alignTop="@+id/imageView10"
        android:layout_toRightOf="@+id/iv_ironTile"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

</RelativeLayout>


